    "cors": "^2.8.5",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "ts-node-dev": "^2.0.0"
  },
  "browserslist": {
    "production": [
